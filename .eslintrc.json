{
	"extends": [
		"next/core-web-vitals",
		"prettier",
		"plugin:prettier/recommended"
	],
	"plugins": [
		"prettier"
	],
	"rules": {
		"indent": [ "error", "tab" ],
		"linebreak-style": [ "error", "unix" ],
		"quotes": [ "error", "double" ],
		"semi": [ "error", "never" ],
		"prettier/prettier": "error"
	}
}
