{
	"name": "viral32111/portal",
	"description": "Landing page for my home network.",
	"keywords": [
		"landing-page",
		"self-hosted",
		"status-page"
	],
	"license": "AGPL-3.0-only",
	"homepage": "https://github.com/viral32111/portal",
	"type": "project",
	"support": {
		"email": "contact@viral32111.com",
		"source": "https://github.com/viral32111/portal",
		"issues": "https://github.com/viral32111/portal/issues",
		"docs": "https://github.com/viral32111/portal"
	},
	"authors": [
		{
			"name": "viral32111",
			"email": "contact@viral32111.com"
		}
	],
	"require": {
		"born05/craft-twofactorauthentication": "3.3.1",
		"craftcms/cms": "^4.5",
		"craftcms/element-api": "3.0.1.1",
		"craftcms/redactor": "3.0.4",
		"nystudio107/craft-code-field": "4.0.10",
		"verbb/default-dashboard": "2.0.0",
		"verbb/super-table": "3.0.9",
		"vlucas/phpdotenv": "^5.5"
	},
	"require-dev": {
		"craftcms/generator": "^1.3.0",
		"yiisoft/yii2-shell": "^2.0.3"
	},
	"autoload": {
		"psr-4": {
			"modules\\": "modules/"
		}
	},
	"config": {
		"allow-plugins": {
			"craftcms/plugin-installer": true,
			"yiisoft/yii2-composer": true
		},
		"sort-packages": true,
		"optimize-autoloader": true
	},
	"repositories": [
		{
			"type": "composer",
			"url": "https://composer.craftcms.com",
			"canonical": false
		}
	]
}